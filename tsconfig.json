{
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig.json to read more about this file */
    "target": "ES5",
    "module": "commonjs",
    "lib": [
      "DOM",
      "es2020.string"
    ],
    "strict": true,
    "outDir": "./dist",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
