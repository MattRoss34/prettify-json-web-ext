{
  "manifest_version": 2,
  "name": "Prettify JSON",
  "version": "0.1.0",
  "description": "Prettify JSON in Kibana and OpenSearch data for easy reading.",
  "browser_specific_settings": {
    "gecko": {
      "id": "{04f203d0-a384-4510-a044-6dd087b9cb03}"
    }
  },
  "permissions": [
    "contextMenus",
    "menus",
    "tabs"
  ],
  "optional_permissions": [
    "<all_urls>"
  ],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icons/inactive.svg",
    "default_title": "Prettify JSON"
  },
  "web_accessible_resources": [
    "assets-manifest.json",
    "runtime.js",
    "contentScript.js",
    "contentScript.css"
  ]
}
